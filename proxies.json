{"$schema": "http://json.schemastore.org/proxies",
  "proxies": {"permanent redirect": {
    "matchCondition": {
      "methods": [
        "GET"
      ],
      "route": "/dashboard"
    },
    "responseOverrides": {
      "response.statusCode": "301",
      "response.headers.Location": "http://localhost:7071/"
      }
    }
  }
}